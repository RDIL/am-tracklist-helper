{
    "compilerOptions": {
        /* Visit https://aka.ms/tsconfig to read more about this file */
        "composite": true,
        "target": "ES2019",
        "lib": ["ES2021", "DOM", "DOM.Iterable"],
        "module": "ES2022",
        "moduleResolution": "NodeNext",
        "allowJs": true,
        "checkJs": false,
        "noEmit": true,
        "forceConsistentCasingInFileNames": true,
        "strictNullChecks": false,
        "strict": true,
        "skipLibCheck": false
    }
}
